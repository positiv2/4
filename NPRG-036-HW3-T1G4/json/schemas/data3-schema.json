{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "maps": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "mapname": {
              "type": "string"
            },
            "spawnlocations": {
              "type": "array",
              "minItems": 2,
              "items": [
                {
                  "type": "object",
                  "properties": {
                    "x1": {
                      "type": "integer"
                    },
                    "x2": {
                      "type": "integer"
                    },
                    "y1": {
                      "type": "integer"
                    },
                    "y2": {
                      "type": "integer"
                    },
                    "z1": {
                      "type": "integer"
                    },
                    "z2": {
                      "type": "integer"
                    }
                  },
                  "required": [
                    "x1",
                    "x2",
                    "y1",
                    "y2",
                    "z1",
                    "z2"
                  ]
                }
              ]
            },
            "plantlocations": {
              "type": "array",
              "minItems": 2,
              "items": [
                {
                  "type": "object",
                  "properties": {
                    "x1": {
                      "type": "integer"
                    },
                    "x2": {
                      "type": "integer"
                    },
                    "y1": {
                      "type": "integer"
                    },
                    "y2": {
                      "type": "integer"
                    },
                    "z1": {
                      "type": "integer"
                    },
                    "z2": {
                      "type": "integer"
                    }
                  },
                  "required": [
                    "x1",
                    "x2",
                    "y1",
                    "y2",
                    "z1",
                    "z2"
                  ]
                }
              ]
            },
            "matches": {
              "type": "array",
              "items": [
                {
                  "type": "object",
                  "properties": {
                    "startdate": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "startdate"
                  ]
                }
              ]
            }
          },
          "required": [
            "mapname",
            "spawnlocations",
            "plantlocations",
            "matches"
          ]
        }
      ]
    }
  },
  "required": [
    "maps"
  ]
}

