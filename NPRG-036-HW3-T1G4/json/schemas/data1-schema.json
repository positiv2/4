{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "users": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "userid": {
              "type": "string"
            },
            "usernames": {
              "type": "array",
              "items": [
                {
                  "type": "string"
                }
              ]
            },
            "email": {
              "type": "string"
            },
            "created": {
              "type": "string",
              "format": "date-time"
            },
            "players": {
              "type": "array",
              "minItems": 1,
              "items": [
                {
                  "type": "object",
                  "properties": {
                    "vgo:username": {
                      "type": "string"
                    },
                    "color": {
                      "type": "string"
                    },
                    "isbot": {
                      "type": "boolean"
                    },
                    "kd": {
                      "type": "number"
                    },
                    "team": {
                      "type": "object",
                      "properties": {
                        "teamname": {
                          "type": "string"
                        }
                      },
                      "required": [
                        "teamname"
                      ]
                    }
                  },
                  "required": [
                    "userid",
                    "vgo:username",
                    "color",
                    "isbot",
                    "kd",
                    "team"
                  ]
                }
              ]
            },
            "matches": {
              "type": "array",
              "items": [
                {
                  "type": "object",
                  "properties": {
                    "startdate": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "startdate"
                  ]
                }
              ]
            },
            "skins": {
              "type": "array",
              "items": [
                {
                  "type": "object",
                  "properties": {
                    "skinname": {
                      "type": "string"
                    },
                    "condition": {
                      "type": "number"
                    }
                  },
                  "required": [
                    "skinname",
                    "condition"
                  ]
                }
              ]
            }
          },
          "required": [
            "usernames",
            "email",
            "created",
            "players"
          ]
        }
      ]
    }
  },
  "required": [
    "users"
  ]
}